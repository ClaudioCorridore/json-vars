{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "semicolon": [
        true,
        "never"
      ],
      "quotemark": [
        true,
        "single"
      ],
      "arrow-parens": [
        true,
        "ban-single-arg-parens"
      ],
      "indent": [
        true,
        "spaces",
        2
      ]
    },
    "rulesDirectory": []
}
